#################################################################################
# PROVIDERS
#################################################################################

provider "aws" {
  region = "us-east-1"
}

#################################################################################
# DATA
#################################################################################

#################################################################################
# OUTPUTS
#################################################################################

output "vpc_id" {
  value = aws_vpc.vpc.id
}

# OwnerID = AWS Account ID
output "vpc_owner_id" {
  value = aws_vpc.vpc.owner_id
}

#################################################################################
# RESOURCES
#################################################################################

resource "aws_vpc" "vpc" {
  cidr_block = "10.0.0.0/16"

  tags = {
    # VPC Name
    "Name" = "VPC1_cidr_10.0.0/16"
  }
}
